!!!!!!!!!!!!!!!!!!
!! rxvt-unicode !!
!!!!!!!!!!!!!!!!!!

URxvt.font: xft:JetBrainsMono Nerd Font:size=11,\
            xft:DejaVu Sans Mono:size=11

URxvt*scrollBar: false
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt*termName: rxvt
URxvt*skipBuiltinGlyphs:    true
URxvt.depth: 32
URxvt.background: [70]#282A36
URxvt.perl-ext-common: default,selection-to-clipboard,matcher,url-select,resize-font,bidi,
URxvt.keysym.Control-Left:    \033[1;5D
URxvt.keysym.Control-Right:    \033[1;5C

!! colors
